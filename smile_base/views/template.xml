<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

    <template id="smile_base_css" name="smile_base_css" inherit_id="web_editor.assets_backend">
      <xpath expr="." position="inside">
        <link rel="stylesheet" href="/smile_base/static/src/css/base.scss"/>
      </xpath>
    </template>

    <template id="login" inherit_id="web.login" name="Login window">
      <xpath expr="//label[@for='login']" position="replace">
        <label for="login">Login</label>
      </xpath>
    </template>

  </data>
</odoo>
